@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.page_wrap .parent_list {
  position: relative;
  padding-right: 50rpx;
  padding-top: 30rpx;
  background-color: #fff;
}
.page_wrap .parent_list .more_icon {
  position: absolute;
  right: 0rpx;
  top: 42rpx;
  color: #FE5B07;
  background-color: #fff;
  border-left: 1px solid #E8E8E8;
  padding-right: 20rpx;
  padding-left: 10rpx;
}
.page_wrap .second_list {
  display: flex;
  height: 89.5vh;
  overflow: hidden;
  align-items: normal;
}
.page_wrap .second_list .left_list {
  width: 25%;
  text-align: center;
}
.page_wrap .second_list .left_list .left_item {
  padding: 40rpx 0;
  justify-content: center;
  color: #666;
  font-size: 28rpx;
}
.page_wrap .second_list .left_list .left_item .rexiao_img {
  width: 20rpx;
  height: 26rpx;
  margin-right: 20rpx;
}
.page_wrap .second_list .left_list .select_child {
  background-color: #fff;
  color: #FE5B07;
  font-size: 28rpx;
  font-weight: bold;
  position: relative;
}
.page_wrap .second_list .left_list .select_child::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  width: 8rpx;
  height: 40rpx;
  background-color: #FE5B07;
  border-radius: 0 20rpx 20rpx 0;
}
.page_wrap .second_list .right_list {
  width: 75%;
  background-color: #fff;
}
.page_wrap .second_list .right_list .list {
  padding-bottom: 30rpx;
}
.page_wrap .all_list_wrap {
  position: fixed;
  top: 90rpx;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
}
.page_wrap .all_list_wrap .all_list {
  background-color: #fff;
  padding: 40rpx 30rpx;
}
.page_wrap .all_list_wrap .all_list .all_top {
  justify-content: space-between;
}
.page_wrap .all_list_wrap .all_list .all_top .all {
  color: #333;
  font-size: 28rpx;
  font-weight: bold;
  padding: 0 30rpx;
  position: relative;
}
.page_wrap .all_list_wrap .all_list .all_top .all::after {
  position: absolute;
  content: '';
  display: block;
  width: 8rpx;
  height: 30rpx;
  background-color: #FE5B07;
  border-radius: 0 20rpx 20rpx 0;
  left: 0;
  top: 6rpx;
}
.page_wrap .all_list_wrap .all_list .all_item {
  display: inline-block;
  padding: 10rpx 26rpx;
  background-color: #F6F6F6;
  border-radius: 4rpx;
  color: #999;
  font-size: 24rpx;
  margin-right: 30rpx;
}
.page_wrap .all_list_wrap .all_list .all_item:nth-child(4n) {
  margin-right: 0;
}
.page_wrap .all_list_wrap .all_list .select_all_item {
  background-color: #fff4ed;
  color: #FE5B07;
}

