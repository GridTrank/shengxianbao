<view class="page_wrap"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list mt20 "><view class="item row "><view class="left ">账号名称</view><view class="right row"><text class="label">{{item.accountName}}</text></view></view><view class="item row"><view class="left ">账户别名</view><view class="right row"><text class="label">{{item.accountTag}}</text></view></view><view class="item row"><view class="left ">联系方式</view><view class="right row"><text class="label">{{item.mobile}}</text></view></view><view class="btn_wrap row"><view data-event-opts="{{[['tap',[['navTo',['./AddOrEdit?pageType=edit&id='+item.id]]]]]}}" class="btn edit row" bindtap="__e"><image src="https://b2bmall2022.oss-cn-hangzhou.aliyuncs.com/edit.png" mode="widthFix"></image><text>编辑</text></view><view data-event-opts="{{[['tap',[['disable',['$0'],[[['dataList','',index]]]]]]]}}" class="btn row" bindtap="__e"><block wx:if="{{item.enabled==1}}"><image src="https://b2bmall2022.oss-cn-hangzhou.aliyuncs.com/shanchu.png" mode="widthFix"></image></block><block wx:else><image src="https://b2bmall2022.oss-cn-hangzhou.aliyuncs.com/qiyong.png" mode="widthFix"></image></block><text>{{item.enabled==1?'禁用':'启用'}}</text></view></view></view></block><view data-event-opts="{{[['tap',[['navTo',['./AddOrEdit?pageType=add']]]]]}}" class="btn_default" bindtap="__e">新建</view></view>